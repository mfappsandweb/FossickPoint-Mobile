<?xml version='1.0' encoding='utf-8'?>
<widget id="com.fossickpoint.safebox" version="0.7.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>fossickpoint-mobile-safebox</name>
    <description>
        FossickPoint Safebox Beta Application.
    </description>
    <author email="ajfle@deakin.edu.au" href="https://mf.nygmarosebeauty.com">
        Deakin University SIT302 Team 17
    </author>
    <content src="login.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="setMediaPlaybackRequiresUserGesture" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
</widget>
